<application>
  <component name="PhpAutoImport" auto_import_file_scope="true" />
  <component name="PhpCSFixer">
    <phpcsfixer_settings>
      <phpcs_fixer_by_interpreter interpreter_id="bdec4c0a-4e1b-401c-8108-e1472ce9649e" standards="PSR1;PSR2;Symfony;DoctrineAnnotation;PHP70Migration;PHP71Migration" tool_path="/root/.composer/vendor/bin/php-cs-fixer" timeout="25000" />
    </phpcsfixer_settings>
  </component>
  <component name="PhpCodeSniffer">
    <phpcs_settings>
      <phpcs_by_interpreter interpreter_id="bdec4c0a-4e1b-401c-8108-e1472ce9649e" standards="MySource;PEAR;PSR1;PSR12;PSR2;Squiz;Zend" tool_path="/root/.composer/vendor/bin/phpcs" timeout="25000" />
